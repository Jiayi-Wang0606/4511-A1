\n","                    documents = \" \".join(topic_data.sample(n=min(5, len(topic_data)), replace=True)['Document'].tolist())\n","\n","                    results.append({\n","                        'date': date,\n","                        'topic': topic,\n","                        'representation': representation,\n","                        'documents': documents\n","                    })\n","\n","\n","results_df = pd.DataFrame(results)\n","results_df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":112},"id":"flYfn9XmSAna","executionInfo":{"status":"ok","timestamp":1713723397263,"user_tz":240,"elapsed":3875,"user":{"displayName":"Jingyi Liu","userId":"05500283357254799815"}},"outputId":"e76aabb4-7300-4777-b534-9eeca9be7d51"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["        date                           topic  \\\n","0 2024-03-08    13_earnings_net_estimates_q4   \n","1 2024-03-24  43_china_commerce_chinese_cook   \n","\n","                                      representation  \\\n","0  ['earnings', 'net', 'estimates', 'q4', 'revenu...   \n","1  ['china', 'commerce', 'chinese', 'cook', 'mini...   \n","\n","                                           documents  \n","0  Why Consulting Service Provider Willdan's Shar...  \n","1  China blocks use of Intel and AMD chips in gov...  "],"text/html":["\n","  <div id=\"df-4aaea64c-a7dd-445d-865a-67e3a48181f0\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>date</th>\n","      <th>topic</th>\n","      <th>representation</th>\n","      <th>documen